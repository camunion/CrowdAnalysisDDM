    "version": "0.2.0",
    "configurations": [
        {
          "type": "gdb",
          "request": "launch",
          "name": "Launch Program",
          "preLaunchTask": "cargo build",
          "target": "./target/debug/code",
          "cwd": "${workspaceRoot}",
          "arguments": "video videos/output/againstWalker1.avi"
        },
      {
        "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "cargo build",
            "program": "${workspaceFolder}/target/debug/code",
            "args": ["video", "videos/output/againstWalker1.avi"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
        "MIMode": "lldb"
      }
    ]
}